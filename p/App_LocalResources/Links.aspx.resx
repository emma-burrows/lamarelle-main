  <data name="maskaradeBlurb.Text" xml:space="preserve">
    <value>Teaching resources to learn French aimed at children.</value>
  </data>
  <data name="maskaradeTitle.Text" xml:space="preserve">
    <value>Maskarade Languages</value>
  </data>
  <data name="multilingualBlurb.Text" xml:space="preserve">
    <value>A website enabling parents speaking several languages to exchange tips and experience on bringing up children.</value>
